<script>
jQuery(document).ready(function($) {
    $("img").mouseenter(function() {
        let $notooltip = $(this).attr("title");
        $(this).attr("notooltip", $notooltip);
        $(this).attr("title", "");
    }).mouseleave(function() {
        let $notooltip = $(this).attr("notooltip");
        $(this).attr("title", $notooltip);
        $(this).removeAttr("notooltip");
    });
});
</script>
